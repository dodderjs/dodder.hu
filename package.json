{
	"name": "dodder-app",
	"version": "0.1.0",
	"description": "",
	"engines": {
		"npm": ">=3"
	},
	"scripts": {
		"start": "webpack-dev-server --hot --config webpack.config.dev.js",
		"start-debug": "webpack-dev-server --hot --config webpack.config.dev.js && node --inspect--brk=9229 index.js",
		"build:prod": "webpack --mode production --config webpack.config.prod.js",
		"build:dev": "webpack -d dist --mode development --config webpack.config.dev.js",
		"test": "jest",
		"analyze-bundle": "babel-node ./tools/analyzeBundle.js",
		"fix": "npx eslint src --fix"
	},
	"author": "dodderjs",
	"license": "ISC",
	"jest": {
		"verbose": true,
		"transform": {
			"\\.(js|jsx)?$": "<rootDir>/tools/testTransform.js"
		},
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
			"\\.(css)$": "<rootDir>/tools/assetsTransformer.js"
		},
		"automock": false,
		"setupFiles": [
			"raf/polyfill",
			"./tools/testSetup.js"
		]
	},
	"devDependencies": {
		"@babel/cli": "7.2.3",
		"@babel/core": "7.5.4",
		"@babel/node": "7.2.2",
		"@babel/plugin-proposal-class-properties": "7.5.0",
		"@babel/plugin-proposal-object-rest-spread": "7.5.4",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-transform-modules-commonjs": "7.5.0",
		"@babel/plugin-transform-runtime": "7.5.0",
		"@babel/preset-env": "7.5.4",
		"@babel/preset-react": "^7.0.0",
		"@hot-loader/react-dom": "^16.8.4",
		"autoprefixer": "9.6.1",
		"babel-core": "7.0.0-bridge.0",
		"babel-eslint": "10.0.2",
		"babel-jest": "^24.5.0",
		"babel-loader": "8.3.0",
		"babel-plugin-dynamic-import-node": "2.3.0",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"browserslist": "4.6.6",
		"connect-history-api-fallback": "^1.6.0",
		"css-loader": "^6.7.3",
		"cssnano": "^4.1.10",
		"enzyme": "3.10.0",
		"enzyme-adapter-react-16": "1.14.0",
		"eslint": "^5.15.1",
		"eslint-config-airbnb": "17.1.1",
		"eslint-config-google": "^0.12.0",
		"eslint-plugin-import": "2.18.0",
		"eslint-plugin-jsx-a11y": "6.2.3",
		"eslint-plugin-react": "7.14.2",
		"file-loader": "^6.2.0",
		"hard-source-webpack-plugin": "0.13.1",
		"html-webpack-plugin": "^5.5.0",
		"husky": "^1.3.1",
		"jest": "^24.5.0",
		"jest-cli": "^24.5.0",
		"jest-fetch-mock": "2.1.2",
		"mini-css-extract-plugin": "2.7.2",
		"minimist": "^1.2.0",
		"node-fetch": "2.3.0",
		"node-sass": "4.12.0",
		"optimize-css-assets-webpack-plugin": "5.0.3",
		"postcss-loader": "^7.0.2",
		"raf": "^3.4.1",
		"redux-mock-store": "^1.5.3",
		"sass-loader": "^13.2.0",
		"style-loader": "^3.3.1",
		"url-loader": "^4.1.1",
		"webpack": "5.75.0",
		"webpack-bundle-analyzer": "3.3.2",
		"webpack-cli": "5.0.1",
		"webpack-dev-server": "3.7.2",
		"webpack-md5-hash": "0.0.6",
		"webpack-pwa-manifest": "4.0.0"
	},
	"dependencies": {
		"@babel/polyfill": "^7.2.5",
		"browser-cookies": "1.2.0",
		"connected-react-router": "6.5.2",
		"history": "^4.9.0",
		"lodash": "4.17.14",
		"normalizr": "3.3.0",
		"path-to-regexp": "3.0.0",
		"prop-types": "^15.7.2",
		"query-string": "6.6.0",
		"react": "^16.8.3",
		"react-dom": "^16.8.3",
		"react-google-login": "5.0.5",
		"react-hot-loader": "4.13.1",
		"react-icons": "3.5.0",
		"react-native-swiper": "^1.5.14",
		"react-redux": "7.1.0",
		"react-router": "5.0.1",
		"react-router-dom": "5.0.1",
		"react-router-prop-types": "1.0.4",
		"redux": "4.0.4",
		"redux-immutable-state-invariant": "^2.1.0",
		"redux-thunk": "^2.3.0"
	},
	"keywords": [],
	"repository": {
		"type": "git",
		"url": ""
	},
	"husky": {
		"hooks": {
			"pre-commit": "npx eslint src && npm run test"
		}
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"babel": {
		"env": {
			"development": {
				"presets": [
					"@babel/preset-env",
					"@babel/preset-react"
				],
				"plugins": [
					"@babel/plugin-syntax-dynamic-import",
					"react-hot-loader/babel",
					"@babel/plugin-proposal-class-properties",
					"@babel/plugin-proposal-object-rest-spread"
				]
			},
			"production": {
				"presets": [
					[
						"@babel/preset-env",
						{
							"es2015": {
								"modules": false
							}
						}
					],
					"@babel/preset-react"
				],
				"plugins": [
					"@babel/plugin-syntax-dynamic-import",
					"@babel/plugin-proposal-class-properties",
					"@babel/plugin-proposal-object-rest-spread"
				]
			},
			"test": {
				"presets": [
					"@babel/preset-env",
					"@babel/preset-react"
				]
			}
		}
	},
	"eslintConfig": {
		"root": true,
		"extends": [
			"eslint:recommended",
			"plugin:import/errors",
			"plugin:import/warnings",
			"plugin:react/recommended"
		],
		"plugins": [
			"react"
		],
		"rules": {
			"indent": [
				"error",
				"tab"
			]
		},
		"parser": "babel-eslint",
		"parserOptions": {
			"ecmaVersion": 6,
			"sourceType": "module",
			"ecmaFeatures": {
				"jsx": true,
				"experimentalObjectRestSpread": true
			}
		},
		"env": {
			"es6": true,
			"browser": true,
			"node": true,
			"jquery": true,
			"jest": true
		},
		"globals": {}
	}
}
